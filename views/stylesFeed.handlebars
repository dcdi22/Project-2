  {{!-- Body Container --}}
  <div class="container mainBody">
    <div class="row">
      <div class="col s12 cards-container">

        {{#each posts}}
          <div class="card cardMain">
            <div class="card-image">
              <img id="productImg" src="{{photoUrl}}">
            </div>
            <div class="card-content">
              <span class="card-title" id="productName">{{title}}</span>
              <p id="productBody">{{body}}</p>

              <h5>Items</h5><h6 style="color:red">We need to style/render this:</h6>
              {{#each Items}}
                <div>
                  <ul>
                    <li>
                      {{brand}}, {{name}}, {{purchaseUrl}}, {{photoUrl}}, {{price}}
                    </li>
                  </ul>
                </div>
              {{/each}}
            </div>
            <div class="card-action">
              <a class="btn-floating btn-large waves-effect waves-light red"><i class="fas fa-praying-hands"></i></a>
              <a class="btn-floating btn-large waves-effect waves-light red"><i class="fas fa-burn"></i></a>
              <a class="btn-floating btn-large waves-effect waves-light red"><i class="fas fa-fire"></i></a>
            </div>
          </div>
        {{/each}}
        
      </div>
    </div>
  </div>