  {{!-- Body Container --}}
  <div class="container mainBody">
    <div class="row">
      <div class="col s12 cards-container">

        {{#each posts}}
          <div class="card cardMain">
            <div class="card-image">
              <img id="productImg" src="{{this.photoUrl}}">
            </div>
            <div class="card-content">
              <span class="card-title" id="productName">{{this.title}}</span>
              <p id="productBody">{{this.body}}</p>
            </div>
            
          </div>
        {{/each}}
        
      </div>
    </div>
  </div>
